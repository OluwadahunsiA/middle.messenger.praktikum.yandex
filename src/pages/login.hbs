{{!< index}}

<div class='login'>
  <h2 class='login__title'>Sign In</h2>
  <form class='login__form'>

    {{#each login }}
    {{> 'input-field/input-field'}}
    {{/each}}

    <button class='login__form-button'>Enter</button>

  </form>
  <p>New here? <a href='./registration.hbs'>Sign up</a> </p>

</div>